-- m >= n -> m = n + (m - n)
--
-- proof by induction on n:
--
--   n = Z: Z + (m - Z) = m - Z    (join)
--                      = m        (lemma)
--
--   n = S n': case m of
--             Z: contradicts m >= n
--             S m': m' = n' + (m' - n')       (by ih using m >= n -> m' >= n')
--                      = n' + (S m' - S n')   (by join)
--                   S n' + (S m' - S n')
--                      = S (n' + (m' - n'))   (by join)
--                   rewrite the first equality in the second.
minus_plus_id : (n:Nat) -> (m:Nat) -> [lt m n = False] -> m = (plus n (minus m n))
ind minus_plus_id n = \m [m_lt_n] . case n [n_eq] of
  Zero    -> conv (conv join -- (join : (plus Zero (minus m Zero)) = minus m Zero)
                   by (lemma_minus_zero_id m)
                   at h . h = (plus Zero (minus m Zero)))
             by (sym Nat Nat n Zero n_eq)
             at n . m = (plus n (minus m n))
  Succ n' -> case m [m_eq] of
    Zero    -> contra (conv join
                       by (conv m_lt_n
                           by m_eq, n_eq
                           at m n. lt m n = False)
                       at h . h = True)
    Succ m' -> let [m'_lt_n'] [_] = conv (conv m_lt_n
                                        by m_eq, n_eq
                                        at m n . lt m n = False)
                                  by (join : lt (Succ m') (Succ n') = lt m' n')
                                  at h . h = False
               in let ih [_] = minus_plus_id n' [ord n_eq] m' [m'_lt_n']
               in let eq1 [_] = (join : Succ (plus n' (minus m' n'))
                                     = plus (Succ n') (minus (Succ m') (Succ n')))
               in let eq2 [_] = conv (conv join by ih at h . Succ m' = Succ h)
                                by eq1 at h . Succ m' = h
               in conv eq2 by (sym Nat Nat m (Succ m') m_eq),
                              (sym Nat Nat n (Succ n') n_eq)
                  at m n . m = (plus n (minus m n))

m_lt_n__Sm_lt_Sn' : (m : Nat) -> (n : Nat) -> lt m n = True -> lt (Succ m) (Succ n) = True
m_lt_n__Sm_lt_Sn' = \m n m_lt_n . conv (m_lt_n__Sm_lt_Sn m n
                                        (conv m_lt_n by (sg m n) at t.t))
                                  by (gs (Succ m) (Succ n))
                                  at t.t

plus_le : (m:Nat) -> (n:Nat) -> (k:Nat) -> le (plus k m) (plus k n) = le m n
plus_le = \m n . ind plus_le k = case k [k_eq] of
  Zero    -> conv join by (sym Nat Nat k Zero k_eq)
             at k . le (plus k m) (plus k n) = le m n
  Succ k' -> let ih [_] = plus_le k' [ord k_eq]
             in let step [_] = (join : le (plus k' m) (plus k' n)
                                     = le (plus (Succ k') m) (plus (Succ k') n))
             in let step' [_] = conv step
                                by (sym Nat Nat k (Succ k') k_eq)
                                at k . le (plus k' m) (plus k' n)
                                     = le (plus k m) (plus k n)
             in conv ih by step' at h . h = le m n

