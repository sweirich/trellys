data Bush : (a:Type) -> Type where
  Nil : Bush a
| Cons : (x:a) -> (y:(Bush (Bush a))) -> Bush a

data WBush : Type where
  W : (Bush WBush) -> WBush

-- kinds
data Kind : Type where
  KStar : Kind
| KArrow : (x:Kind) -> (y:Kind) -> Kind

-- sequences of kinds
data Sig : Type where
  SEps : Sig -- empty sequence
| SCons : (x:Kind) -> (xs:Sig) -> Sig

-- signature corresponding to the declaration of Bush and WBush
type deltaW : Sig
prog deltaW =
  (SCons (KArrow KStar KStar) (SCons KStar SEps))

-- typed de Bruijn variables  
data Var : (s:Sig)(k:Kind) -> Type where
  VZ : Var (SCons k s) k
| VS : (j:Kind) -> (v:Var s k) -> Var (SCons j s) k -- not sure where j is introduced in the defn?

-- polynomial type expressions representing the RHS of a datatype declaration
data Ty : (d:Sig)(l:Sig)(k:Kind) -> Type where
  D : (v:Var d k) -> Ty d l k
| V : (v:Var l k) -> Ty d l k
| Dot : (j:Kind) -> (f:Ty d l (KArrow j k)) -> (x:Ty d l j) -> Ty d l k -- j?
| Zero : Ty d l KStar
| One : Ty d l KStar  
| Sum : (s:Ty d l KStar) -> (t:Ty d l KStar) -> Ty d l KStar
| Prod : (s:Ty d l KStar) -> (t:Ty d l KStar) -> Ty d l KStar

