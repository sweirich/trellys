data Lam : (m:Nat) -> Type where
  LVar : (n:Nat) -> (x : Fin n) -> Lam n
| LApp : (n:Nat) -> (f : Lam n) -> (a : Lam n) -> Lam n
| LLam : [n:Nat] -> [u:S n = m] -> (t : Lam m) -> Lam n

