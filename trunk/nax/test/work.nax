data L2: (i -> i -> *) ->
         (i -> i -> *) -> (i -> i -> *) where
  LNil  : L2 x r i i
  LCons : x i j -> r j k -> L2 x r i k
    deriving fixpoint List2

data Unit = One

data Elem: * -> Unit -> Unit -> * where
  E: a -> Elem a {One} {One} 

data Seq a r s = 
  Con (r a) (s a)
    
axiom id : a -> a

